<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Ada Administration Page</title>
    @Styles.Render("~/bundle/css")
</head>
<body>
    @Html.Partial("_MainMenu")
    <div class="container body-content">
        @RenderBody()

        <footer class="col-md-12 col-sm-12 col-xs-12">
            <hr />
            <p>&copy; @DateTime.Now.Year - Ada Administration Page</p>
        </footer>
    </div>

    @Scripts.Render("~/bundle/script")
    @Scripts.Render("~/bundle/moment")
    @RenderSection("scripts", required: false)
</body>
</html>